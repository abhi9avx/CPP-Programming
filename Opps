// defien the car class 

class Car{
    //properties and attributes 
    String brand;
    String color;
    int price ;
    
    
    //Method or function 
    
    void displayCarInfo(){
        System.out.println("Brand:" + brand);
        System.out.println("Color:" + color);
        System.out.println("Price:" + price);
    }
}

//Main Class to tets the Car Class 

public class Main {
    public static void main(String[] args){
        //Craete a object of the car class 
        Car myCar = new Car();
        
        
        //set Properties of the car 
        
        myCar.brand = "Thar";
        myCar.color = "Black";
        myCar.price = 1000000;
        
        //Call the method to display the car info 
        myCar.displayCarInfo();
        
        //Creating a new car class 
        Car myNewCar = new Car();
        //setting the properties of new car 
        myNewCar.brand = "Scarpio";
        myNewCar.color = "Pink";
        myNewCar.price = 25000000;
        
        // Calling the mehod to display the new car info 
        myNewCar.displayCarInfo();
    }
}

........

  java -cp /tmp/cpXylm2ci7/Main
Brand:Thar
Color:Black
Price:1000000
Brand:Scarpio
Color:Pink
Price:25000000

